# sem_daemon
service sem_daemon /system/bin/sem_daemon
    user root
    group root nfc
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib:/system/lib64:/system/lib:/sbin
    oneshot
    disabled
    seclabel u:r:recovery:s0

on early-boot
    mkdir /efs/sec_efs/esek 0775 root root
    start sem_daemon

